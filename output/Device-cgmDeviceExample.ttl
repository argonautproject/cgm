# - resource -------------------------------------------------------------------

<http://argo.run/cgm/Device/cgmDeviceExample> a fhir:Device ;
  fhir:nodeRole fhir:treeRoot ;
  fhir:id [ fhir:v "cgmDeviceExample"] ; # 
  fhir:identifier ( [
     fhir:system [ fhir:v "http://acme.com/devices"^^xsd:anyURI ] ;
     fhir:value [ fhir:v "456789" ]
  ] ) ; # 
  fhir:serialNumber [ fhir:v "ABC123"] ; # 
